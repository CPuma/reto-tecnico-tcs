{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": ".build",
    "moduleResolution": "node",
    "lib": [
      "es2015",
      "ESNext"
    ],
    "rootDir": "./",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  }
}